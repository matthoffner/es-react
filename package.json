{
  "name": "es-react-rc",
  "version": "17.0.0-rc.1",
  "description": "An ES6 module exposing the latest version of react and react-dom",
  "module": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "react",
    "es",
    "module",
    "import",
    "export"
  ],
  "repository": "https://www.github.com/matthoffner/es-react-rc",
  "bugs": {
    "url": "https://github.com/matthoffner/es-react-rc/issues"
  },
  "scripts": {
    "build": "rollup -c scripts/rollup.config.js",
    "clean": "rimraf *.js dev",
    "prepare-dev": "node ./scripts/write-dev-pkgjson.js",
    "prepublishOnly": "run-s clean build prepare-dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "15.7.2",
    "react": "17.0.0-rc.1",
    "react-dom": "17.0.0-rc.1",
    "react-is": "17.0.0-rc.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-is": "^16.7.1"
  }
}
